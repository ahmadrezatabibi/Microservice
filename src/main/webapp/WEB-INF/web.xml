<?xml version="1.0" encoding="UTF-8"?>
<javaee:web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:javaee="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="JavaWebappCodingTest" version="3.0">
  <javaee:display-name>java-webapp-microservice</javaee:display-name>
  <javaee:context-param>
    <javaee:param-name>contextConfigLocation</javaee:param-name>
    <javaee:param-value>/WEB-INF/webContext.xml</javaee:param-value>
  </javaee:context-param>
  <javaee:context-param>
    <javaee:param-name>parentContextKey</javaee:param-name>
    <javaee:param-value>app.context</javaee:param-value>
  </javaee:context-param>
  <javaee:listener>
    <javaee:listener-class>org.springframework.web.context.ContextLoaderListener</javaee:listener-class>
  </javaee:listener>
</javaee:web-app>